{
  "manifest_version": 2,

  "name": "Reading List",
  "description": "Creates a reading list with the best content across the web.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "http://localhost:8080"
  ]
}
